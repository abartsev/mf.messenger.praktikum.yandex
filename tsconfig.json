{
    "compilerOptions": {
        "esModuleInterop": true,
        "target": "es2016",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "declaration": false,
        "strictNullChecks": true,
        "outDir": "src/js",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "alwaysStrict": true,
    },
}