{
    "extends": "xo",
    "env": {
        "jest": true,
        "browser": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
        "max-len": [2, 120],
        "@typescript-eslint/no-unused-vars": 2,
        "no-mixed-spaces-and-tabs": 0
    }
}